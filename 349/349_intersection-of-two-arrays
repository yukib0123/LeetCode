'''C++
class Solution {
public:
    vector<int> intersection(vector<int>& nums1, vector<int>& nums2) {
        set<int> nums1_set(nums1.begin(), nums1.end());
        vector<int> meet;
        for (int element : nums2) {
            if (nums1_set.find(element) != nums1_set.end()) {
                meet.push_back(element);
                nums1_set.erase(element);
            }
        }
        return meet;
    }
};
'''
